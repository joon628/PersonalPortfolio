# Strapi Environment Variables
# Copy this file to .env and fill in your own values

# Application keys (comma-separated)
APP_KEYS=generate-key-1,generate-key-2,generate-key-3,generate-key-4

# API Token Salt
API_TOKEN_SALT=generate-random-salt

# Admin JWT Secret
ADMIN_JWT_SECRET=generate-random-admin-secret

# Transfer Token Salt
TRANSFER_TOKEN_SALT=generate-random-transfer-salt

# JWT Secret
JWT_SECRET=generate-random-jwt-secret

# Host and Port
HOST=0.0.0.0
PORT=1337

# Node Environment
NODE_ENV=production
